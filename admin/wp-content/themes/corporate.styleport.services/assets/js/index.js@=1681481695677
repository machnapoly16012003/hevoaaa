.geometryParm.heightSegments),e=new z.FIo({vertexShader:"attribute vec3 position;\r\n\r\nuniform mat4 modelViewMatrix;\r\nuniform mat4 projectionMatrix;\r\n\r\nattribute vec2 uv;\r\nvarying vec2 vUv;\r\n\r\nvoid main(){\r\n\tvUv = uv;\r\n\tgl_Position = projectionMatrix * modelViewMatrix * vec4(position, 1.0);\r\n}\r\n",fragmentShader:"// precision mediump float;\r\nprecision lowp float;\r\n\r\nvarying vec2 vUv;\r\nuniform vec2 mouse;\r\nuniform float time;\r\nuniform vec2 resolution;\r\n\r\n#define PI 3.14159265359\r\n\r\n//\r\n// Description : Array and textureless GLSL 2D simplex noise function.\r\n//      Author : Ian McEwan, Ashima Arts.\r\n//  Maintainer : stegu\r\n//     Lastmod : 20110822 (ijm)\r\n//     License : Copyright (C) 2011 Ashima Arts. All rights reserved.\r\n//               Distributed under the MIT License. See LICENSE file.\r\n//               https://github.com/ashima/webgl-noise\r\n//               https://github.com/stegu/webgl-noise\r\n//\r\n\r\nvec3 mod289(vec3 x) {\r\n\treturn x - floor(x * (1.0 / 289.0)) * 289.0;\r\n}\r\n\r\nvec2 mod289(vec2 x) {\r\n\treturn x - floor(x * (1.0 / 289.0)) * 289.0;\r\n}\r\n\r\nvec3 permute(vec3 x) {\r\n\treturn mod289(((x * 34.0) + 10.0) * x);\r\n}\r\n\r\nfloat snoise(vec2 v) {\r\n\tconst vec4 C = vec4(0.211324865405187,  // (3.0-sqrt(3.0))/6.0\r\n\t0.366025403784439,  // 0.5*(sqrt(3.0)-1.0)\r\n\t-0.577350269189626,  // -1.0 + 2.0 * C.x\r\n\t0.024390243902439); // 1.0 / 41.0\r\n// First corner\r\n\tvec2 i = floor(v + dot(v, C.yy));\r\n\tvec2 x0 = v - i + dot(i, C.xx);\r\n\r\n// Other corners\r\n\tvec2 i1;\r\n  //i1.x = step( x0.y, x0.x ); // x0.x > x0.y ? 1.0 : 0.0\r\n  //i1.y = 1.0 - i1.x;\r\n\ti1 = (x0.x > x0.y) ? vec2(1.0, 0.0) : vec2(0.0, 1.0);\r\n  // x0 = x0 - 0.0 + 0.0 * C.xx ;\r\n  // x1 = x0 - i1 + 1.0 * C.xx ;\r\n  // x2 = x0 - 1.0 + 2.0 * C.xx ;\r\n\tvec4 x12 = x0.xyxy + C.xxzz;\r\n\tx12.xy -= i1;\r\n\r\n// Permutations\r\n\ti = mod289(i); // Avoid truncation effects in permutation\r\n\tvec3 p = permute(permute(i.y + vec3(0.0, i1.y, 1.0)) + i.x + vec3(0.0, i1.x, 1.0));\r\n\r\n\tvec3 m = max(0.5 - vec3(dot(x0, x0), dot(x12.xy, x12.xy), dot(x12.zw, x12.zw)), 0.0);\r\n\tm = m * m;\r\n\tm = m * m;\r\n\r\n// Gradients: 41 points uniformly over a line, mapped onto a diamond.\r\n// The ring size 17*17 = 289 is close to a multiple of 41 (41*7 = 287)\r\n\r\n\tvec3 x = 2.0 * fract(p * C.www) - 1.0;\r\n\tvec3 h = abs(x) - 0.5;\r\n\tvec3 ox = floor(x + 0.5);\r\n\tvec3 a0 = x - ox;\r\n\r\n// Normalise gradients implicitly by scaling m\r\n// Approximation of: m *= inversesqrt( a0*a0 + h*h );\r\n\tm *= 1.79284291400159 - 0.85373472095314 * (a0 * a0 + h * h);\r\n\r\n// Compute final noise value at P\r\n\tvec3 g;\r\n\tg.x = a0.x * x0.x + h.x * x0.y;\r\n\tg.yz = a0.yz * x12.xz + h.yz * x12.yw;\r\n\treturn 130.0 * dot(m, g);\r\n}\r\n\r\n//\r\n// main\r\n//\r\n\r\n//\r\n// 重ねてグラデにするためのオブジェクトを生成\r\n// drawcCircle\r\n// float d1:中心X\r\n// float d2:中心Y\r\n// float r1:半径X\r\n// float r2:半径Y\r\n// float r:半径長さ\r\n// float fade:フェード長さ\r\n//\r\nfloat drawcCircle(float d1, float d2, float r1, float r2, float r, float fade) {\r\n\tfloat t = time / 2.0;\r\n\r\n\tfloat circle = 0.0;\r\n\r\n\t// sin 0~1に\r\n\tfloat percent = (sin(t) + 1.0) * 0.5;\r\n\r\n\tvec2 st = gl_FragCoord.xy / vec2(r1, r2);\r\n\r\n    // 中心位置\r\n\tfloat attenuate = distance(st, vec2(d1, d2));\r\n\r\n    // 縁をフェード\r\n\t// 経過時間でフェード距離を動かす\r\n\tcircle = smoothstep((percent * percent * 0.1 + 0.1) + attenuate + fade, attenuate, r);\r\n\r\n\treturn circle;\r\n}\r\n\r\nmat2 rotate2d(float _angle) {\r\n\treturn mat2(cos(_angle), -sin(_angle), sin(_angle), cos(_angle));\r\n}\r\n\r\nvoid main() {\r\n\t// vec2 st = gl_FragCoord.xy / resolution.xy;\r\n\r\n\t////////////////////\r\n\t// color\r\n\t////////////////////\r\n\t// 背景色(#f8f8f8)\r\n\tvec3 colorBase = vec3(0.973, 0.973, 0.973);\r\n\r\n\t// グラデ 背景色(#3e839b)\r\n\t// vec3 colorGrad_Base = vec3(0.243, 0.514, 0.608) * 1.65;\r\n\tvec3 colorGrad_Base = vec3(0.216, 0.678, 0.682) * 1.5;\r\n\r\n\t// グラデ 基本色(#3e839b)\r\n\t// vec3 colorGrad_Default = vec3(0.243, 0.514, 0.608) * 1.65;\r\n\r\n\t// グラデ 濃い色#355288)\r\n\tvec3 colorGrad_Dark = vec3(0.208, 0.322, 0.533) * 1.65;\r\n\r\n\t// グラデ 薄1.0#37adae)\r\n\tvec3 colorGrad_Light = vec3(0.216, 0.678, 0.682) * 1.5;\r\n\r\n\t// デバッグ用 赤\r\n\tvec3 colorGrad_DEBUG = vec3(1.0, 0.0, 0.0);\r\n\r\n\t////////////////////\r\n\t// parameter\r\n\t////////////////////\r\n\t// 時間経過\r\n\tfloat timeGrad = time * 1.0;\r\n\r\n\t// 円運動\r\n\tvec2 circular = vec2(cos(timeGrad), sin(timeGrad));\r\n\r\n\t// ノイズ生成\r\n\tfloat noise = snoise(vec2(length(circular), length(gl_FragCoord) / length(resolution))) * 1.0;\r\n\r\n\t// 半径\r\n\tfloat r = min(resolution.x, resolution.y) * 0.35 + min(resolution.x, resolution.y) * noise * 0.015;\r\n\r\n\t// 中心位置を動かす\r\n\tfloat translate = sin(timeGrad) * 2.0 * noise;\r\n\r\n\t////////////////////\r\n\t// BLUE\r\n\t////////////////////\r\n\t// 配置位置\r\n\t\t// 1 2 3\r\n\t\t// 4 5 6\r\n\t\t// 7 8 9\r\n\t// // オブジェクト 01\r\n\t// float obj01 = drawcCircle(((resolution.x / r * (0.5 - 0.3)) + (circular.x * 0.4) + (translate + 0.0)), ((circular.y / r * (0.5 - 0.3)) + (circular.y * 0.9) + 0.5), r * 0.5, r * 1.5, 2.0, 2.0);\r\n\t// vec3 layer01 = mix(colorGrad_Default, colorBase, obj01);\r\n\r\n\t// // オブジェクト 02\r\n\t// float obj02 = drawcCircle(((resolution.x / r * (0.5 + 0.0)) + (circular.x * 0.5) + (translate + 0.0)), ((circular.y / r * (0.5 - 0.3)) - (circular.y * 0.8) + 0.5), r * 0.6, r * 1.6, 1.0, 1.2);\r\n\t// vec3 layer02 = mix(colorGrad_Light, colorBase, obj02);\r\n\r\n\t// // オブジェクト 03\r\n\t// float obj03 = drawcCircle(((resolution.x / r * (0.5 + 0.3)) + (circular.x * 0.6) + (translate + 0.0)), ((circular.y / r * (0.5 - 0.3)) + (circular.y * 0.4) + 0.5), r * 1.7, r * 1.7, 2.0, 2.0);\r\n\t// vec3 layer03 = mix(colorGrad_Dark, colorBase, obj03);\r\n\r\n\t// // オブジェクト 04\r\n\t// float obj04 = drawcCircle(((resolution.x / r * (0.5 - 0.3)) + (circular.x * 0.7) + (translate + 0.0)), ((circular.y