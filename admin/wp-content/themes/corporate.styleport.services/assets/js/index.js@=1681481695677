5 - 0.3)) + ((circular.x * 0.4 * 0.5) + (cos(timeGrad * 0.7) + 0.0))), ((circular.y / r * (0.5 - 0.3)) + ((circular.y * 0.9 * 0.5) + (sin(timeGrad * 0.6) + 0.0))), r * 0.5, r * 1.5, 2.0 * 1.5, 1.0);\r\n\tvec3 layer01 = mix(colorGrad_Light, colorBase, obj01);\r\n\r\n\t// オブジェクト 02\r\n\tfloat obj02 = drawcCircle(((resolution.x / r * (0.5 + 0.0)) + ((circular.x * 0.5 * 0.5) + (cos(timeGrad * 1.0) + 0.0))), ((circular.y / r * (0.5 - 0.3)) - ((circular.y * 0.8 * 0.5) + (sin(timeGrad * 1.0) + 0.0))), r * 0.6, r * 1.6, 1.0 * 1.5, 1.0);\r\n\tvec3 layer02 = mix(colorGrad_Light, colorBase, obj02);\r\n\r\n\t// オブジェクト 03\r\n\tfloat obj03 = drawcCircle(((resolution.x / r * (0.5 + 0.3)) + ((circular.x * 0.6 * 0.5) + (cos(timeGrad * 1.3) + 0.0))), ((circular.y / r * (0.5 - 0.3)) + ((circular.y * 0.4 * 0.5) + (sin(timeGrad * 1.3) + 0.0))), r * 1.7, r * 1.7, 2.0 * 1.5, 1.0);\r\n\tvec3 layer03 = mix(colorGrad_Light, colorBase, obj03);\r\n\r\n\t// オブジェクト 04\r\n\tfloat obj04 = drawcCircle(((resolution.x / r * (0.5 - 0.3)) + ((circular.x * 0.7 * 0.5) + (cos(timeGrad * 0.8) + 0.0))), ((circular.y / r * (0.5 + 0.0)) - ((circular.y * 0.5 * 0.5) + (sin(timeGrad * 0.8) + 0.0))), r * 0.8, r * 1.8, 1.0 * 1.5, 1.0);\r\n\tvec3 layer04 = mix(colorGrad_Dark, colorBase, obj04);\r\n\r\n\t// オブジェクト 05\r\n\tfloat obj05 = drawcCircle(((resolution.x / r * (0.5 + 0.0)) + ((circular.x * 0.8 * 0.5) + (cos(timeGrad * 1.0) + 0.0))), ((circular.y / r * (0.5 + 0.0)) + ((circular.y * 0.6 * 0.5) + (sin(timeGrad * 1.0) + 0.0))), r * 0.9, r * 1.9, 1.0 * 1.5, 1.0);\r\n\tvec3 layer05 = mix(colorGrad_Light, colorBase, obj05);\r\n\r\n\t// オブジェクト 06\r\n\tfloat obj06 = drawcCircle(((resolution.x / r * (0.5 + 0.3)) + ((circular.x * 0.9 * 0.5) + (cos(timeGrad * 1.2) + 0.0))), ((circular.y / r * (0.5 + 0.0)) - ((circular.y * 0.7 * 0.5) + (sin(timeGrad * 1.2) + 0.0))), r * 0.8, r * 1.8, 2.0 * 1.5, 2.0);\r\n\tvec3 layer06 = mix(colorGrad_Light, colorBase, obj06);\r\n\r\n\t// オブジェクト 07\r\n\tfloat obj07 = drawcCircle(((resolut